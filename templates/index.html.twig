{% extends "base.html.twig" %}
{% block body %}
    Hello world!
    <script>
        const route = "{{ path('read_all_calendar_events')|escape('js') }}";
        console.log(route);
        $.get(route, function (data) {
            $(".result").html(data);
        })
        const route2 = "{{ path('read_calendar_event_by_id', {calendarEventId: '1'})|escape('js') }}";
        $.get(route2, function (data) {
            $(".result").html(data);
        });
    </script>
{% endblock %}